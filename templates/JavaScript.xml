<templateSet group="JavaScript">
  <template name="req" value="const $NAME$ = require('$NAME$');$END$" description="inserts require() expression" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="keys" value="Object.keys($OBJECT$).forEach((key) =&gt; {&#10;  let item = $OBJECT$[key];&#10;  &#10;  $END$&#10;});" description="iterate Object.keys" toReformat="true" toShortenFQNames="true">
    <variable name="OBJECT" expression="jsSuggestVariableName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="it" value="it('should $TEXT$', function($DONE$) {&#10;  $DONE$$END$&#10;});" description="Insert it should for test" toReformat="true" toShortenFQNames="true">
    <variable name="TEXT" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="DONE" expression="enum(&quot;&quot;, &quot;done&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="des" value="describe('# $DESCR$', function() {&#10;&#10;  $END$&#10;&#10;});" description="describe for test" toReformat="true" toShortenFQNames="true">
    <variable name="DESCR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="request" value="request(http.createServer(app.callback()))&#10;  .$METHOD$('/$PATH$')&#10;  .end((err, res) =&gt; {&#10;     $END$&#10;  });" description="insert request for test" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD" expression="enum(&quot;get&quot;,&quot;post&quot;,&quot;put&quot;,&quot;delete&quot;)" defaultValue="get" alwaysStopAt="true" />
    <variable name="PATH" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="us" value="'use strict';$END$" description="Inserts 'use strict' statement" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="throw" value="throw new Error('$ERROR$');$END$" description="throw block" toReformat="true" toShortenFQNames="true">
    <variable name="ERROR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="if" value="if ($COND$) {&#10;  $END$&#10;}" description="if" toReformat="false" toShortenFQNames="true">
    <variable name="COND" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="while" value="let $VAR$ = 0;&#10;&#10;while ($VAR$ &lt; $OBJECT$.length) {&#10;  let item = $OBJECT$[$VAR$];&#10;  &#10;  $END$&#10;  $VAR$++;&#10;}" description="while loop" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="OBJECT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>